{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./path.json",
  "compilerOptions": {
    "outDir": "dist",
    "declarationDir": "dist",
    "target": "es6",
    "lib": ["es2016", "dom"],
    "jsx": "react",
    "strict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
